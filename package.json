        "browserify": "^14.3.0",
        "css-loader": "^0.28.11",
        "eslint": "^4.3.0",
        "eslint-loader": "^1.9.0",
        "event-stream": "^3.3.4",
        "extract-text-webpack-plugin": "^3.0.0",
        "html-webpack-exclude-assets-plugin": "0.0.7",
